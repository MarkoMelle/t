{
  "name": "popover",
  "version": "1.0.0",
  "description": "An example repo for running puppeteer in github actions ci environment",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": "src/index.html",
  "scripts": {
    "start": "yarn parcel src/index.html -public-url ./  --port 9999",
    "build": "yarn parcel build src/index.html --public-url ./ --no-source-maps",
    "e2e": "yarn jest",
    "lint": "eslint .",
    "coverage": "nyc yarn e2e --coverage"
  },
  "author": "",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "babel-plugin-istanbul": "^5.2.0",
    "jest": "^27.2.5",
    "jest-puppeteer": "^6.0.0",
    "jest-puppeteer-istanbul": "^0.5.3",
    "nyc": "^15.1.0",
    "parcel": "^2.0.0-rc.0",
    "puppeteer": "^10.4.0"
  },
  "dependencies": {
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5"
  }
}
